<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
     http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="testChangeMethod" class="com.gzy.spring.entityCode.childProperties.replacedMethod.TestChangeMethod">
        <replaced-method name="changeMe" replacer="replacer"></replaced-method>
    </bean>

    <bean id="replacer" class="com.gzy.spring.entityCode.childProperties.replacedMethod.TestMethodReplacer"></bean>





</beans>